    android:id="@+id/activity_users"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.comfest.cf18.UsersActivity">

    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/allusertoolbar"
        android:id="@+id/user_list"></android.support.v7.widget.RecyclerView>

    <include layout="@layout/app_bar_layout"

        android:id="@+id/allusertoolbar" />

</RelativeLayout>
